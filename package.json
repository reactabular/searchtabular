{
  "name": "searchtabular",
  "version": "1.3.2",
  "description": "Search utilities",
  "scripts": {
    "test:all": "npm run test:lint && npm test",
    "test": "jest --",
    "test:coverage": "jest --coverage --",
    "test:lint": "eslint . --ext .js --cache",
    "test:watch": "jest --watch --",
    "dist:build": "rimraf ./dist && babel ./src --out-dir ./dist",
    "preversion": "npm run test:all && npm run dist:build && git commit --allow-empty -am \"Update dist\""
  },
  "main": "dist",
  "repository": {
    "type": "git",
    "url": "https://github.com/reactabular/searchtabular.git"
  },
  "keywords": [
    "search",
    "searching",
    "reactabular"
  ],
  "files": [
    "dist"
  ],
  "jest": {
    "collectCoverage": true,
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ]
  },
  "author": "Juho Vepsalainen <bebraw@gmail.com> (http://github.com/bebraw)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reactabular/searchtabular/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2 || ^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "git-prepush-hook": "^1.0.1",
    "jest": "^18.0.0",
    "lodash": "^4.17.2",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.4.1",
    "rimraf": "^2.5.4"
  },
  "peerDependencies": {
    "lodash": ">= 3.0.0 < 5.0.0",
    "react": ">= 15.0.0 < 16.0.0"
  },
  "pre-push": [
    "test:all"
  ]
}
